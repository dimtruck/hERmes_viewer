<?xml version="1.0" encoding="UTF-8"?>

<validators xmlns='http://openrepose.org/repose/validator/v1.0'>
   <validator
      role="atomhopper"
      default="true"
      wadl="usage-schema/atom_hopper.wadl"
      check-well-formed="true"
      remove-dups="true"
      check-xsd-grammar="true"
      check-elements="true"
      xpath-version="2"
      check-plain-params="true"
      do-xsd-grammar-transform="true"
      enable-pre-process-extension="true"
      xsl-engine="XalanC"
      dot-output="validator.dot"
      join-xpath-checks="true"/>
</validators>
