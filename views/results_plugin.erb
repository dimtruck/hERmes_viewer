<!DOCTYPE html>
<html>
  <head>
    <title><%= app_detail[:title] %> - Past Results</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Bootstrap -->
    <link href="/css/bootstrap.min.css" rel="stylesheet" media="screen">
    <link href="/css/repose.css" rel="stylesheet" media="screen">
    <link href="/css/prettify.css" rel="stylesheet" media="screen">
  </head>
  <body>
    <div class="navbar navbar-inverse navbar-fixed-top">
      <button class="navbar-toggle" type="button" data-toggle="collapse" data-target=".repose-navbar-collapse">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="/"><%= app_detail[:title] %></a>
      <div class="nav-collapse collapse bs-navbar-collapse">
        <ul class="nav navbar-nav">
          <li><a href="/tests">Running Tests</a></li>
          <li><a href="/applications">Applications</a></li>
          <li class="active"><a href="/results">Results</a></li>
        </ul>
      </div>
    </div>
    <div class="container">
      <ul class="breadcrumb">
        <li><a href="/">Home</a></li>
        <li><a href="/results">Results</a></li>
        <li><a href="/results/<%= app_detail[:app_id] %>"><%= app_detail[:app_id] %></a></li>
        <li><a href="/results/<%= app_detail[:app_id] %>/<%= app_detail[:test_type] %>"><%= app_detail[:test_type] %></a></li>
        <li class="active"><%= app_detail[:plugin_name] %> - <%= app_detail[:option] %></li>
      </ul>
      <div class="panel panel-summary">
        <div class="panel-heading">
          <h3 class="panel-title">Summary Test Results</h3>
        </div>
        <table class="table table-hover">
<% app_detail[:summary_plugin_data].each do |key, value| %>
          <caption>Repose (Origin only)</caption>
          <thead>
            <tr>
              <th>instance</th>
  <% value[:headers].each do |header| %>
              <th><%= header %></th>
  <% end %>
            </tr>
          </thead>
          <tbody>
  <% value[:content].each do |instance, data| %>
            <tr>
              <td><%= instance %></td>
     <% data.each do |point| %>
              <td><%= point %></td>
     <% end %>
            </tr>
  <% end %>
          </tbody>
<% end %>
        </table>
      </div>
<% app_detail[:detailed_plugin_data].each do |key, value| %>
  <% value[:content].each do |instance, data| %>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title"><%= key %> - <%= instance %></h3>
        </div>        
        <div id="<%= key %>_<%= instance.gsub(/[.-]/,'_') %>" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
  <% end %>
<% end %>
<% app_detail[:detailed_plugin_data].each do |key, value| %>
  <% value[:content].each do |instance, data| %>
      <div class="panel panel-summary">
        <div class="panel-heading">
          <h3 class="panel-title">Detailed Test Results for <%= instance %></h3>
        </div>
        <table class="table table-hover">
          <caption>Repose (Origin only)</caption>
          <thead>
            <tr>
              <th>Time</th>
    <% value[:headers].each do |header| %>
              <th><%= header %></th>
    <% end %>
            </tr>
          </thead>
          <tbody>
    <% data.each do |time, entry| %>
            <tr>
              <td><%= time %></td>
      <% entry.each do |p| %>
              <td><%= p %></td>
      <% end %>
            </tr>
    <% end %>
          </tbody>
        </table>
      </div>
  <% end %>
<% end %>
    </div>
    <hr>
    <footer>
      <p>&copy; Rackspace Hosting 2013</p>
    </footer>
  </div> <!-- /container -->
    <!-- JavaScript plugins (requires jQuery) -->
    <script src="http://code.jquery.com/jquery.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="/js/bootstrap.min.js"></script>

    <!-- Enable responsive features in IE8 with Respond.js (https://github.com/scottjehl/Respond) -->
    <script src="/js/respond.js"></script>
    <script src="/js/repose.min.js"></script>
    <script src="http://code.highcharts.com/highcharts.js"></script>
    <script src="/js/prettify.js"></script>
    <script type="text/javascript">
      $(document).ready(function() {
        window.prettyPrint && prettyPrint()
        $(".accordion-toggle").collapse();
<% app_detail[:detailed_unordered_plugin_data].each do |key, value| %>
  <% value[:content].each do |instance, data| %>
      var option_<%= key %>_<%= instance.gsub(/[.-]/,'_') %> = {
          chart: {
            renderTo: '<%= key %>_<%= instance.gsub(/[.-]/,'_') %>',
            type: 'area',
          },
          title: {
            text: '<%= key %> <%= instance %>'
          },
          yAxis:{
            title: {
              text: 'measurement here'
            },
            min: 0
          },
          series: [
    <% value[:headers].each do |header| %>
          {
            name: '<%= header %>'
          },
    <% end %>
          ]
      }
    <% data.each_index do |index| %>
      option_<%= key %>_<%= instance.gsub(/[.-]/,'_') %>.series[<%= index %>].data = <%= data[index].map {|t| [t[:time],t[:value].to_f ]}.to_json %>;
    <% end %>
    var <%= key %>_<%= instance.gsub(/[.-]/,'_') %>_chart = new Highcharts.Chart(option_<%= key %>_<%= instance.gsub(/[.-]/,'_') %>);
  <% end %>
<% end %>
      });
    </script>
  </body>
</html>
