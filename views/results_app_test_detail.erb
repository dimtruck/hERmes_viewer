<!DOCTYPE html>
<html>
  <head>
    <title>Repose - Applications</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Bootstrap -->
    <link href="/css/bootstrap.min.css" rel="stylesheet" media="screen">
    <link href="/css/repose.css" rel="stylesheet" media="screen">
  </head>
  <body>
    <div class="navbar navbar-inverse navbar-fixed-top">
      <button class="navbar-toggle" type="button" data-toggle="collapse" data-target=".repose-navbar-collapse">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="/">Repose Perf Tests</a>
      <div class="nav-collapse collapse bs-navbar-collapse">
        <ul class="nav navbar-nav">
          <li><a href="/tests">Running Tests</a></li>
          <li><a href="/applications">Applications</a></li>
          <li class="active"><a href="/results">Results</a></li>
        </ul>
      </div>
    </div>
    <div class="container">
      <ul class="breadcrumb">
        <li><a href="/">Home</a></li>
        <li><a href="/results">Results</a></li>
        <li><a href="/results/<%= app_detail.app_id %>"><%= app_detail.name %></a></li>
        <li class="active"><%= app_detail.test_type %></li>
      </ul>
      <div class="panel panel-summary">
        <div class="panel-heading">
          <h3 class="panel-title">Test Results</h3>
        </div>
        <form method="POST">
          <table class="table table-hover">
            <thead>
              <tr>
                <th>Tag</th>
                <th>Date</th>
                <th>ART overhead</th>
                <th>Throughput overhead</th>
                <th>Error overhead</th>
                <th>Compare</th>
              </tr>
            </thead>
            <tbody>
<% app_detail.result_set_list.each do |result| %>
              <tr>
                <td><%= result.tag %></td>
                <td><a href="/results/<%= app_detail.app_id %>/<%= app_detail.test_type %>/id/<%= result.id %>/date/<%= result.date %>"><%= DateTime.strptime(result.date.chomp("000"), '%s') %></td>
                <td><%= result.avg %> ms</td>
                <td><%= result.throughput %> rps</td>
                <td><%= result.errors %> total errors</td>
                <td>
                  <div class="checkbox">
                    <label>
                      <input type="checkbox" name="compare[]" value="<%=result.id %>">
                    </label>
                  </div>
                </td>
              </tr>
              <tr>
                <td colspan="6">
                  <div class="accordion" id="config-accordion-<%= result.id %>">
<% result.network_results.each do |metric_key, metric_data| %>
                    <div class="accordion-group">
                      <div class="accordion-heading">
                        <a class="accordion-toggle" data-toggle="collapse" data-parent="#config-accordion-<%= result.id %>" href="#<%= metric_key %>-<%= result.id %>"><%= metric_key %></a>
                      </div>
                      <div id="<%= metric_key %>-<%= result.id %>" class="accordion-body collapse in">
                        <div class="accordion-inner">
                          <table class="table table-hover">
                            <thead>
                              <tr>
                                <th>
                                  Device 
                                </th>
                              <% metric_data[:headers].each do |key| %>
                                <th><%= key %></th>
                              <% end %>
                              </tr>
                            </thead>
                            <tbody>
                              <% metric_data[:content].each do |key,results| %>
                              <tr>
                                <td><%= key %></td>
                                <% results.each do |r| %>
                                <td><%= r %></td>
                                <% end %>
                              </tr>
                              <% end  %>
                            </tbody>
                          </table>
                        </div>
                      </div>
                    </div>
<% end %>
                  </div>
                </td>
              </tr>
<% end %>
            </tbody>
            <tfoot>
              <tr>
                <td colspan="6">
                  <button type="submit" class="btn btn-primary">Compare Tests</button>
                </td>
              </tr>
            </tfoot>
          </table>
        </form>
      </div>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title">Average Response Time Overhead</h3>
        </div>
        <div id="art" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title">Error Overhead</h3>
        </div>
        <div id="cpu" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title">Throughput Overhead</h3>
        </div>
        <div id="memory" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title">GC Metrics</h3>
          <h5>GarbageCollectorImpl.PSMarkSweep.CollectionTime, GarbageCollectorImpl.PSMarkSweep.CollectionCount, GarbageCollectorImpl.PSScavenge.CollectionTime, GarbageCollectorImpl.PSScavenge.CollectionCount</h5>
        </div>
        <div id="gc" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title">Memory Pool Metrics</h3>
          <h5>MemoryPoolImpl.CodeCache.Usage_committed, MemoryPoolImpl.CodeCache.Usage_init, MemoryPoolImpl.CodeCache.Usage_max, MemoryPoolImpl.CodeCache.Usage_used</h5>
        </div>
        <div id="code_cache" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title">Heap Memory Metrics</h3>
          <h5>MemoryImpl.HeapMemoryUsage_committed, MemoryImpl.HeapMemoryUsage_init, MemoryImpl.HeapMemoryUsage_max, MemoryImpl.HeapMemoryUsage_used</h5>
        </div>
        <div id="heap_memory_usage" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title">NonHeap Memory Metrics</h3>
          <h5>MemoryImpl.NonHeapMemoryUsage_committed, MemoryImpl.NonHeapMemoryUsage_init, MemoryImpl.NonHeapMemoryUsage_max, MemoryImpl.NonHeapMemoryUsage_used</h5>
        </div>
        <div id="non_heap_memory_usage" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title">OS Metrics</h3>
          <h5>UnixOperatingSystem.OpenFileDescriptorCount, UnixOperatingSystem.MaxFileDescriptorCount, UnixOperatingSystem.CommittedVirtualMemorySize, UnixOperatingSystem.TotalSwapSpaceSize, UnixOperatingSystem.FreeSwapSpaceSize, UnixOperatingSystem.ProcessCpuTime, UnixOperatingSystem.FreePhysicalMemorySize, UnixOperatingSystem.TotalPhysicalMemorySize, UnixOperatingSystem.SystemLoadAverage</h5>
        </div>
        <div id="os" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title">Threading Metrics</h3>
          <h5>ThreadImpl.PeakThreadCount, ThreadImpl.DaemonThreadCount, ThreadImpl.ThreadCount, ThreadImpl.TotalStartedThreadCount</h5>
        </div>
        <div id="threading" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
    </div>
    <hr>
    <footer>
      <p>&copy; Rackspace Hosting 2013</p>
    </footer>
  </div> <!-- /container -->
    <!-- JavaScript plugins (requires jQuery) -->
    <script src="http://code.jquery.com/jquery.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="/js/bootstrap.min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/moment.js/2.1.0/moment.min.js"></script>

    <!-- Enable responsive features in IE8 with Respond.js (https://github.com/scottjehl/Respond) -->
    <script src="/js/respond.js"></script>
    <script src="/js/repose.min.js"></script>
    <script src="http://code.highcharts.com/highcharts.js"></script>
    <script type="text/javascript">
      $(document).ready(function() {
        var art_option = {
          chart: {
            renderTo: 'art',
            defaultSeriesType: 'spline',
          },
          title: {
            text: 'Repose Average Response Time Overhead'
          },
          yAxis:{
            title: {
              text: 'Milliseconds'
            }
          },
          xAxis:{
            type: 'datetime',
            dateTimeLabelFormats: {
                day: '%H:%M:%S'
            }
          },
          series: [{
            name: 'Average Response Time overhead'
          }]
        };        
        var memory_option = {
          chart: {
            renderTo: 'memory',
            defaultSeriesType: 'spline',
          },
          title: {
            text: 'Throughput Overhead'
          },
          yAxis:{
            title: {
              text: '%'
            }
          },
          xAxis:{
            type: 'datetime',
            dateTimeLabelFormats: {
                day: '%a %d %b %H:%M:%S'
            }
          },
          series: [{
            name: 'Average throughput overhead'
          }]
        };        
        var cpu_option = {
          chart: {
            renderTo: 'cpu',
            defaultSeriesType: 'spline',
          },
          title: {
            text: 'Error Overhead'
          },
          yAxis:{
            title: {
              text: '%'
            }
          },
          xAxis:{
            type: 'datetime',
            dateTimeLabelFormats: {
                day: '%Y-%b-%d'
            }
          },
          series: [{
            name: 'Error overhead'
          }]
        };        

        $.getJSON('/results/<%= app_detail.app_id %>/<%= app_detail.test_type %>/metric/avg', function(data) {
          var data2 = data.map(convertDate);
          art_option.series[0].data = data2;
          var art_chart = new Highcharts.Chart(art_option);
        }); 
        $.getJSON('/results/<%= app_detail.app_id %>/<%= app_detail.test_type %>/metric/throughput', function(data) {
          var data2 = data.map(convertDate);
          memory_option.series[0].data = data2;
          var memory_chart = new Highcharts.Chart(memory_option);
        }); 
        $.getJSON('/results/<%= app_detail.app_id %>/<%= app_detail.test_type %>/metric/errors', function(data) {
          var data2 = data.map(convertDate);
          cpu_option.series[0].data = data2;
          var cpu_chart = new Highcharts.Chart(cpu_option);
        }); 
      });

      function convertDate(value){
        var temp_date = new Date(parseInt(value[0]));
 console.log(temp_date);
        return [parseInt(value[0]), Math.abs(value[1])];
      }
    </script>
  </body>
</html>
