<!DOCTYPE html>
<html>
  <head>
    <title>Repose - Applications</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Bootstrap -->
    <link href="/css/bootstrap.min.css" rel="stylesheet" media="screen">
    <link href="/css/repose.css" rel="stylesheet" media="screen">
  </head>
  <body>
    <div class="navbar navbar-inverse navbar-fixed-top">
      <button class="navbar-toggle" type="button" data-toggle="collapse" data-target=".repose-navbar-collapse">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="/">Repose Perf Tests</a>
      <div class="nav-collapse collapse bs-navbar-collapse">
        <ul class="nav navbar-nav">
          <li><a href="/RunningTests">Running Tests</a></li>
          <li><a href="/applications">Applications</a></li>
          <li class="active"><a href="/results">Results</a></li>
        </ul>
      </div>
    </div>
    <div class="container">
      <ul class="breadcrumb">
        <li><a href="/">Home</a></li>
        <li><a href="/results">Results</a></li>
        <li><a href="<%= app_detail.href %>"><%= app_detail.name %></a></li>
        <li class="active"><%= app_detail.test_type %></li>
      </ul>
      <div class="panel panel-summary">
        <div class="panel-heading">
          <h3 class="panel-title">Test Results</h3>
        </div>
        <table class="table table-hover">
          <thead>
            <tr>
              <th>Date</th>
              <th>ART overhead</th>
              <th>Network overhead</th>
              <th>CPU overhead</th>
              <th>Memory overhead</th>
              <th>Disk overhead</th>
            </tr>
          </thead>
          <tbody>
            <% app_detail.result_set_list.each do |result| %>
            <tr>
              <td><a href="<%= app_detail.href %>/<%= app_detail.test_type %>/date/<%= result.date %>"><%= result.date %></td>
              <td><%= result.art %> ms</td>
              <td><%= result.network %>%</td>
              <td><%= result.cpu %>%</td>
              <td><%= result.memory %>%</td>
              <td><%= result.disk %>%</td>
            </tr>
            <% end %>
          </tbody>
        </table>
      </div>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title">Average Response Time Overhead</h3>
        </div>
        <div id="art" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title">Network Utilization Overhead</h3>
        </div>
        <div id="network" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title">CPU Utilization Overhead</h3>
        </div>
        <div id="cpu" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title">Memory Utilization Overhead</h3>
        </div>
        <div id="memory" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title">Disk Utilization Overhead</h3>
        </div>
        <div id="disk" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
    </div>
    <hr>
    <footer>
      <p>&copy; Rackspace Hosting 2013</p>
    </footer>
  </div> <!-- /container -->
    <!-- JavaScript plugins (requires jQuery) -->
    <script src="http://code.jquery.com/jquery.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="/js/bootstrap.min.js"></script>

    <!-- Enable responsive features in IE8 with Respond.js (https://github.com/scottjehl/Respond) -->
    <script src="/js/respond.js"></script>
    <script src="/js/repose.min.js"></script>
    <script src="http://code.highcharts.com/highcharts.js"></script>
    <script type="text/javascript">
      $(document).ready(function() {
        var art_option = {
          chart: {
            renderTo: 'art',
            defaultSeriesType: 'spline',
          },
          title: {
            text: 'Repose Average Response Time Overhead'
          },
          yAxis:{
            title: {
              text: 'Milliseconds'
            },
            min: 0
          },
          xAxis:{
            type: 'datetime',
            dateTimeLabelFormats: {
                day: '%Y-%b-%d'
            }
          },
          series: [{
            name: 'Average Response Time overhead'
          }]
        };        
        var memory_option = {
          chart: {
            renderTo: 'memory',
            defaultSeriesType: 'spline',
          },
          title: {
            text: 'Memory Utilization Overhead'
          },
          yAxis:{
            title: {
              text: '%'
            },
            min: 0
          },
          xAxis:{
            type: 'datetime',
            dateTimeLabelFormats: {
                day: '%Y-%b-%d'
            }
          },
          series: [{
            name: 'Average memory utilization overhead'
          }]
        };        
        var disk_option = {
          chart: {
            renderTo: 'disk',
            defaultSeriesType: 'spline',
          },
          title: {
            text: 'Disk Utilization Overhead'
          },
          yAxis:{
            title: {
              text: '%'
            },
            min: 0
          },
          xAxis:{
            type: 'datetime',
            dateTimeLabelFormats: {
                day: '%Y-%b-%d'
            }
          },
          series: [{
            name: 'Average disk utilization overhead'
          }]
        };        
        var cpu_option = {
          chart: {
            renderTo: 'cpu',
            defaultSeriesType: 'spline',
          },
          title: {
            text: 'CPU Utilization Overhead'
          },
          yAxis:{
            title: {
              text: '%'
            },
            min: 0
          },
          xAxis:{
            type: 'datetime',
            dateTimeLabelFormats: {
                day: '%Y-%b-%d'
            }
          },
          series: [{
            name: 'Average cpu utilization overhead'
          }]
        };        
        var network_option = {
          chart: {
            renderTo: 'network',
            defaultSeriesType: 'spline',
          },
          title: {
            text: 'Network Utilization Overhead'
          },
          yAxis:{
            title: {
              text: '%'
            },
            min: 0
          },
          xAxis:{
            type: 'datetime',
            dateTimeLabelFormats: {
                day: '%Y-%b-%d'
            }
          },
          series: [{
            name: 'Average network utilization overhead'
          }]
        };       

        $.getJSON('<%= app_detail.href %>/<%= app_detail.test_type %>/metric/art', function(data) {
          var data2 = data.map(convertDate);
          art_option.series[0].data = data2;
          var art_chart = new Highcharts.Chart(art_option);
        }); 
        $.getJSON('<%= app_detail.href %>/<%= app_detail.test_type %>/metric/memory', function(data) {
          var data2 = data.map(convertDate);
          memory_option.series[0].data = data2;
          var memory_chart = new Highcharts.Chart(memory_option);
        }); 
        $.getJSON('<%= app_detail.href %>/<%= app_detail.test_type %>/metric/cpu', function(data) {
          var data2 = data.map(convertDate);
          cpu_option.series[0].data = data2;
          var cpu_chart = new Highcharts.Chart(cpu_option);
        }); 
        $.getJSON('<%= app_detail.href %>/<%= app_detail.test_type %>/metric/disk', function(data) {
          var data2 = data.map(convertDate);
          disk_option.series[0].data = data2;
          var disk_chart = new Highcharts.Chart(disk_option);
        }); 
        $.getJSON('<%= app_detail.href %>/<%= app_detail.test_type %>/metric/network', function(data) {
          var data2 = data.map(convertDate);
          network_option.series[0].data = data2;
          var network_chart = new Highcharts.Chart(network_option);
        }); 
      });

      function convertDate(value){
        var date_data = value[0].split(',');
        var temp_date = Date.UTC(parseInt(date_data[0]),parseInt(date_data[1]),parseInt(date_data[2]));
        return [temp_date, value[1]];
      }
    </script>
  </body>
</html>
