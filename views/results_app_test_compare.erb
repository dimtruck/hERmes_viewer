<!DOCTYPE html>
<html>
  <head>
    <title><%= app_detail[:title] %> - Results - <%= app_detail[:test_type] %> - Compare</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Bootstrap -->
    <link href="/css/bootstrap.min.css" rel="stylesheet" media="screen">
    <link href="/css/repose.css" rel="stylesheet" media="screen">
  </head>
  <body>
    <div class="navbar navbar-inverse navbar-fixed-top">
      <button class="navbar-toggle" type="button" data-toggle="collapse" data-target=".repose-navbar-collapse">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="/"><%= app_detail[:title] %></a>
      <div class="nav-collapse collapse bs-navbar-collapse">
        <ul class="nav navbar-nav">
          <li><a href="/tests">Running Tests</a></li>
          <li><a href="/applications">Applications</a></li>
          <li class="active"><a href="/results">Results</a></li>
        </ul>
      </div>
    </div>
    <div class="container">
      <ul class="breadcrumb">
        <li><a href="/">Home</a></li>
        <li><a href="/results">Results</a></li>
        <li><a href="/results/<%= app_detail[:app_id] %>"><%= app_detail[:app_id] %></a></li>
        <li class="active"><%= app_detail[:test_type] %></li>
      </ul>
      <div class="panel panel-summary">
        <div class="panel-heading">
          <h3 class="panel-title">Test Results</h3>
        </div>
          <table class="table table-hover">
            <caption>Execution via Repose is listed first.   Execution against origin service directly is in parentheses</caption>
            <thead>
              <tr>
                <th>Tag</th>
                <th>Date</th>
                <th>ART</th>
                <th>Throughput</th>
                <th>Error</th>
              </tr>
            </thead>
            <tbody>
<% app_detail[:compared_result_set_list].each do |key, result_list| %>
              <tr>
                <td><%= result_list[0]['tag']  %></td>
                <td><a href="/results/<%= app_detail[:app_id] %>/<%= app_detail[:test_type] %>/id/<%= key %>"><%= result_list[0][:length].to_f %>(<%= result_list[1][:length].to_f %>) - difference of <%= result_list[0][:length].to_f - result_list[1][:length].to_f %> s</a></td>
                <td><%= result_list[0][:average].to_f %> ms (<%= result_list[1][:average].to_f %> ms) - difference of <%= result_list[0][:average].to_f - result_list[1][:average].to_f %> ms</td>
                <td><%= result_list[0][:throughput].to_f %> rps (<%= result_list[1][:throughput].to_f %> rps) - difference of <%= result_list[0][:throughput].to_f - result_list[1][:throughput].to_f %> rps</td>
                <td><%= result_list[0][:errors].to_f %> total errors (<%= result_list[1][:errors].to_f %> total errors) - difference of <%= result_list[0][:errors].to_f - result_list[1][:errors].to_f %> total errors</td>
              </tr>
<% end %>
            </tbody>
          </table>
      </div>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title">Average Response Time Overhead</h3>
        </div>
        <div id="art" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title">Error Overhead</h3>
        </div>
        <div id="cpu" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title">Throughput Overhead</h3>
        </div>
        <div id="memory" style="min-width: 310px; height: 400px; margin: 0 auto;"></div>
      </div>
    </div>
    <hr>
    <footer>
      <p>&copy; Rackspace Hosting 2013</p>
    </footer>
  </div> <!-- /container -->
    <!-- JavaScript plugins (requires jQuery) -->
    <script src="http://code.jquery.com/jquery.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="/js/bootstrap.min.js"></script>

    <!-- Enable responsive features in IE8 with Respond.js (https://github.com/scottjehl/Respond) -->
    <script src="/js/respond.js"></script>
    <script src="/js/repose.min.js"></script>
    <script src="http://code.highcharts.com/highcharts.js"></script>
    <script type="text/javascript">
      $(document).ready(function() {
        var art_option = {
          chart: {
            renderTo: 'art',
            defaultSeriesType: 'spline',
          },
          title: {
            text: 'Repose Average Response Time Overhead'
          },
          yAxis:{
            title: {
              text: 'Milliseconds'
            }
          },
          series: [
            {name: 'Compare 1 - Average Response Time'},
            {name: 'Compare 2 - Response Time overhead'}
          ]
        };        
        var throughput_option = {
          chart: {
            renderTo: 'memory',
            defaultSeriesType: 'spline',
          },
          title: {
            text: 'Throughput Overhead'
          },
          yAxis:{
            title: {
              text: 'Requests/sec'
            }
          },
          series: [
            {name: 'Compare 1 - Average Response Time'},
            {name: 'Compare 2 - Response Time overhead'}
          ]
        };        
        var error_option = {
          chart: {
            renderTo: 'cpu',
            defaultSeriesType: 'spline',
          },
          title: {
            text: 'Error Overhead'
          },
          yAxis:{
            title: {
              text: 'Total'
            }
          },
          series: [
            {name: 'Compare 1 - Average Response Time'},
            {name: 'Compare 2 - Response Time overhead'}
          ]
        };   

        var results = <%= app_detail[:compared_result_set_list].map {|key,result_list| key } %>.join(",");     

        $.getJSON('/results/<%= app_detail[:app_id] %>/<%= app_detail[:test_type] %>/metric/avg/compare/' + results, function(data) {
console.log(data);
          art_option.series[0].data = data.compare_one;
          art_option.series[1].data = data.compare_two;
          var art_chart = new Highcharts.Chart(art_option);
        }); 
        $.getJSON('/results/<%= app_detail[:app_id] %>/<%= app_detail[:test_type] %>/metric/throughput/compare/' + results, function(data) {
console.log(data);
          throughput_option.series[0].data = data.compare_one;
          throughput_option.series[1].data = data.compare_two;
          var throughput_chart = new Highcharts.Chart(throughput_option);
        }); 
        $.getJSON('/results/<%= app_detail[:app_id] %>/<%= app_detail[:test_type] %>/metric/errors/compare/' + results, function(data) {
console.log(data);
          error_option.series[0].data = data.compare_one;
          error_option.series[1].data = data.compare_two;
          var error_chart = new Highcharts.Chart(error_option);
        }); 
      });

      function convertDate(value){
        var temp_date = new Date(parseInt(value[0])).toUTCString();
 console.log(temp_date);
        return [temp_date, value[1]];
      }
    </script>
  </body>
</html>
